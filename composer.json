{
    "name": "meteor/country",
    "description": "Meteor countries",
    "keywords": [
        "laravel",
        "country",
        "countries"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/meteorid-labs/laravel-country/issues",
        "source": "https://github.com/meteorid-labs/laravel-country"
    },
    "authors": [
        {
            "name": "Aslam H",
            "email": "aslam.hafidz@meteor.id"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Meteor\\Country\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meteor\\Country\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meteor\\Country\\NegaraServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
